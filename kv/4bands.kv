<FourRingsScreen>:
	BoxLayout:
		orientation:"vertical"
		spacing:5
		padding:[5, 5, 5, 5]
		Image:
			pos_hint: {'top': 1, 'center_y': .5, 'center_x': .5}
			source: "data/images/resistor_teal.png" if app.theme_cls.primary_palette == 'Teal' else "data/images/resistor_blue.png"			
			size_hint: None, None
			size: self.texture_size
			canvas.after:
				Color:
					rgba:root.BUTTON_COLORS[root.resistor_colors[0]]
				Rectangle:
					pos: (self.x+15, self.y)
					size: (15, 60)
				Color:
					rgba:root.BUTTON_COLORS[root.resistor_colors[1]]
				Rectangle:
					pos: (self.x+55, self.y+3)
					size: (15, 52)
				Color:
					rgba:root.BUTTON_COLORS[root.resistor_colors[2]]
				Rectangle:
					pos: (self.x+91, self.y+4)
					size: (15, 51)
				Color:
					rgba:root.BUTTON_COLORS[root.resistor_colors[3]]
				Rectangle:
					pos: (self.x+130, self.y)
					size: (15, 60)
		MDLabel:
			text:root.result
			font_style:'Title'
			theme_text_color:'Primary'
			halign:'center'
			size_hint_y:None
			height:20
